            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color1"
            android:text="안드로이드 개발자" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color1"
